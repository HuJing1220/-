/* components/search/search.wxss */
.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.searchBox {

  background-color: white;
  border-radius: 10em;
  display: flex;
  flex-direction: row;
  align-items: center;

}
.searchBox image {
  height: 25px;
  width: 25px;
  margin-left: 1px;
}
.searchBox input {

  margin-left: 1px;
}
.placeholder {
  font-size: 12px;
  color: black;
}
